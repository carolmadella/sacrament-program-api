<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Data Table</title>
</head>
<body>
  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Title</th>
        <th>Reference</th>
        <th>Link</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(item => { %>
        <tr>
          <td><%= item.date %></td>
          <td><%= item.title %></td>
          <td><%= item.reference %></td>
          <td><a href="<%= item.link %>">Link</a></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
